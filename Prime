// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n,r,flag;
    printf("Enter");
    scanf ("%d",&n);
    for(int i=2;i<n;i++)
    {
        r=(n%i);
        if(r==0)
        {
            flag++;
        }
    }
    if(flag==0)
    {
        printf ("prime");
    }
    else
    {
        printf ("not prime");
    }

    return 0;
}
